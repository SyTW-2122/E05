{
  "exclude": [
    "node_modules",
    "backend"
  ],
  "compilerOptions": {
    
    
    "target": "es2020",
    "lib": ["dom", "es2020"],
    "types": ["cypress"],

    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    // "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": [
    "./src",
    "node_modules/cypress/types/*",
    "cypress/**/*.ts"
  ]
}